      "label": "Real-time performance monitoring"
    }
  ],
  
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View cluster overview and metrics",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/icons/shortcut-dashboard.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Models",
      "short_name": "Models",
      "description": "Manage AI models",
      "url": "/models",
      "icons": [
        {
          "src": "/icons/shortcut-models.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Monitoring",
      "short_name": "Monitor",
      "description": "Performance monitoring and alerts",
      "url": "/monitoring",
      "icons": [
        {
          "src": "/icons/shortcut-monitoring.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Security",
      "short_name": "Security",
      "description": "Zero-trust security dashboard",
      "url": "/security",
      "icons": [
        {
          "src": "/icons/shortcut-security.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  
  "related_applications": [
    {
      "platform": "web",
      "url": "https://ollama.com/"
    }
  ],
  
  "prefer_related_applications": false,
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "file_handlers": [
    {
      "action": "/import-model",
      "accept": {
        "application/x-ollama-model": [".ollama", ".gguf"],
        "application/json": [".json"]
      }
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "ollama",
      "url": "/protocol-handler?url=%s"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "model",
          "accept": ["application/x-ollama-model", ".ollama", ".gguf"]
        }
      ]
    }
  }
}