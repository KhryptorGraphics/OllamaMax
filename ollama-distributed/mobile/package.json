{
  "name": "ollama-max-mobile",
  "version": "1.0.0",
  "description": "OllamaMax Mobile App - React Native",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace OllamaMax.xcworkspace -scheme OllamaMax -configuration Release -destination generic/platform=iOS -archivePath OllamaMax.xcarchive archive",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "pod-install": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^13.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-device-info": "^10.11.0",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^18.3.2",
    "@react-native-firebase/messaging": "^18.3.2",
    "@react-native-firebase/analytics": "^18.3.2",
    "react-native-webview": "^13.6.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-code-push": "^8.2.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-share": "^9.4.1",
    "react-native-permissions": "^3.9.3",
    "react-native-background-job": "^0.2.9",
    "react-native-websocket": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native"
  },
  "keywords": [
    "react-native",
    "mobile",
    "ios",
    "android",
    "ollama",
    "ai",
    "distributed-systems",
    "monitoring"
  ],
  "author": "OllamaMax Team",
