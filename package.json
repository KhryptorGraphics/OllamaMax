{
  "name": "ollamamax",
  "version": "1.0.0",
  "description": "Enterprise-Grade Distributed AI Model Platform",
  "main": "index.js",
  "scripts": {
    "build": "go build -o bin/ollamamax .",
    "test": "npx playwright test",
    "test:api": "npx playwright test tests/api-health-tests.js",
    "test:ui": "npx playwright test tests/ui-interaction-tests.js",
    "test:p2p": "npx playwright test tests/p2p-model-migration-tests.js",
    "test:comprehensive": "npx playwright test tests/comprehensive-test-strategy.js",
    "test:quick": "node tests/run-comprehensive-tests.js quick",
    "test:p2p-only": "node tests/run-comprehensive-tests.js p2p",
    "test:full": "node tests/run-comprehensive-tests.js full",
    "test:cross-browser": "node tests/run-comprehensive-tests.js crossBrowser",
    "test:performance": "npx playwright test --config=playwright.performance.config.js",
    "test:performance:comprehensive": "npx playwright test tests/performance-comprehensive.test.js --config=playwright.performance.config.js",
    "test:performance:stress": "npx playwright test tests/performance-stress.test.js --config=playwright.performance.config.js",
    "test:performance:monitoring": "npx playwright test tests/performance-monitoring.test.js --config=playwright.performance.config.js",
    "test:performance:all": "node tests/run-performance-tests.js",
    "test:performance:report": "node tests/performance-analysis-report.js",
    "test:coverage": "go test -coverprofile=coverage.out ./... && go tool cover -html=coverage.out -o coverage.html",
    "test:show-report": "npx playwright show-report",
    "lint": "golangci-lint run",
    "dev": "go run .",
    "build:distributed": "cd ollama-distributed && go build -o bin/ollama-distributed ./cmd/distributed-ollama",
    "test:distributed": "cd ollama-distributed && go test ./... -v",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "deploy:backend": "docker-compose -f docker-compose.backend.yml up -d",
    "smart-agents": "node .claude-flow/commands/smart-agents/index.js",
    "agents": "./smart-agents",
    "agents:status": "node .claude-flow/commands/smart-agents/index.js status",
    "agents:test": "node .claude-flow/commands/smart-agents/test-runner.js"
  },
  "keywords": [
    "ai",
    "ollama",
    "distributed",
    "kubernetes",
    "microservices"
  ],
  "author": "OllamaMax Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khryptorgraphics/ollamamax.git"
  },
  "bugs": {
    "url": "https://github.com/khryptorgraphics/ollamamax/issues"
  },
  "homepage": "https://github.com/khryptorgraphics/ollamamax#readme",
  "dependencies": {
    "playwright": "^1.55.0"
  }
}